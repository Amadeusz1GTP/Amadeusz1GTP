    #include <iostream>
#include <cstdlib>
#include <ctime>
int wczytajLiczbe()
{
    srand( time( NULL ) );
    std::cout << "Wylosowanie pierwsze: " <<(( std::rand() % 10 ) + 50 ) << std::endl;
    int liczba =( std::rand() % 10 ) + 50;

int main()
    std::cout << "Podaj liczbe: ";
    int liczba = wczytajLiczbe();
    std::cout << "Podales liczbe: " << liczba << std::endl;

  
    return 0;
}
