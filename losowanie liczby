#include <iostream>
#include <windows.h>
#include <time.h>
using namespace std;

int liczba;

int main(int argc, char** argv)

{

	cout<<"ROZPOCZYNAMY LOSOWANIE";
	Sleep(1000);
	cout<<endl;

	srand(time(NULL));

	for (int i=1;i<=6;i++)
	{
		liczba = rand()%49+1;
		cout<<liczba<<"\a"<<endl;

	}

	cout<<"jesli trafiles te liczby to wygrales 100000000 $";




	return 0;
}
