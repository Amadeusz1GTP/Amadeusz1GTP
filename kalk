#include <iostream>
#include <bitset>

using namespace std;
    int main()
    {
            int a;
            int b;

        cout <<"podaj liczbe"<<endl;
        cin >> a; 
        cout <<"podaj liczbe"<<endl;
        cin>> b;

        cout << bitset<8>(a|b).to_string();
        cout << bitset<8>(a&b).to_string();
        cout << bitset<8>(a^b).to_string();
        

        return 0;
   
}
