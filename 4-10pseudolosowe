#include <iostream>
#include <ctime>
#include <math.h>

using namespace std;

int main(){
int tablica[999],i,suma;
srand( time( NULL ) );
suma=0;
for (i=0;i<=998;i++){
tablica[i] = ( rand() % 7 ) + 4;
cout << tablica[i] << endl;
suma +=tablica[i];
}
cout << "Suma liczb pseudolosowych wynosi: " << suma << endl;
return 0;
}
